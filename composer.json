{
    "name": "fm-labs/cakephp-google-recaptcha",
    "description": "GoogleRecaptcha plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "GoogleRecaptcha\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoogleRecaptcha\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
